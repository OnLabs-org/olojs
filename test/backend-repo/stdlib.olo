<% __render__ = require "markdown" %>
<% test = (import "./import").test %>

# olojs stdlib test
The <% test("green",TRUE) %> tests are passed, whilte the <% test("red", FALSE) %>
are failed.

### markdown
<% markdown = require "markdown" %>
<% slice = (s, a, b) -> str(map (i -> s(i)) (a:b)) %>
* <% test("markdown(text)", slice(markdown("*text*"),0,19) == "<p><em>text</em></p>") %>

### path
<% path = require "path" %>
* <% test("path.getBaseName", path.getBaseName("/path/to/doc.txt") == "doc.txt") %>
* <% test("path.getDirName", path.getDirName("/path/to/doc.txt") == "/path/to") %>
* <% test("path.getExtName", path.getExtName("/path/to/doc.txt") == ".txt") %>
* <% test("path.normalize", path.normalize("/path/to/x/.././doc.txt") == "/path/to/doc.txt") %>
* <% test("path.resolve", path.resolve("/path/to/doc.txt", "../../doc.json") == "/path/doc.json") %>

### math
<% math = require "math" %>
* <% test("math.round", math.round(25.33) == 25 & math.round(25.98) == 26)%>
* <% test("math.ceil", math.ceil(25.33) == 26) %>
* <% test("math.floor", math.floor(25.98) == 25) %>
* <% test("math.trunc", math.trunc(25.33) == 25 & math.trunc(25.98) == 25)%>
* <% test("math.max", math.max(3,2,1,120, -200) == 120) %>
* <% test("math.min", math.min(3,2,1,120, -200) == -200) %>
* <% test("math.random", math.random() > 0 & math.random() < 1) %>
* <% test("math.sqrt", math.sqrt(25) == 5) %>
* <% test("math.E", math.round(math.E*1000) == 2718)%>
* <% test("math.exp", math.round(math.exp(3)*1000) == 20086)%>
* <% test("math.log", math.round(math.log(3)*1000) == 1099)%>
* <% test("math.log10", math.round(math.log10(100)*1000) == 2000)%>
* <% test("math.PI", math.round(math.PI*100) == 314)%>
* <% test("math.abs", math.abs(-3) == 3 & math.abs(3) == 3)%>
* <% test("math.sin", math.round(math.sin(math.PI/3)*1000) == 866)%>
* <% test("math.asin", math.round(math.asin(3^0.5/2)*1000) == 1047)%>
* <% test("math.cos", math.round(math.cos(math.PI/3)*1000) == 500)%>
* <% test("math.acos", math.round(math.acos(0.5)*1000) == 1047)%>
* <% test("math.tan", math.round(math.tan(math.PI/4)*1000) == 1000)%>
* <% test("math.atan", math.round(math.atan(1)*1000) == 785)%>
* <% test("math.sinh", math.round(math.sinh(3)*1000) == 10018)%>
* <% test("math.asinh", math.round(math.asinh(3)*1000) == 1818)%>
* <% test("math.cosh", math.round(math.cosh(3)*1000) == 10068)%>
* <% test("math.acosh", math.round(math.acosh(3)*1000) == 1763)%>
* <% test("math.tanh", math.round(math.tanh(3)*1000) == 995)%>
* <% test("math.atanh", math.round(math.atanh(0.3)*1000) == 310)%>
